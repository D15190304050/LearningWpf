<Window x:Class="CommandBindings.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommandBindings"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <Grid>
        <StackPanel Margin="5" Button.Click="ButtonClick">
            <Button>TestNewCommand</Button>
            <Button>SimpleDocument</Button>
            <Button>CustomCommand</Button>
            <Button>NoCommandTextBox</Button>
            <Button>CostumControlWithCommand</Button>
            <Button>TwoTextBox</Button>
            <Button>MonitorCommands</Button>
        </StackPanel>
    </Grid>
</Window>
