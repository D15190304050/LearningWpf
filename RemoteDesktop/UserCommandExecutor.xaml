<Window x:Class="RemoteDesktop.UserCommandExecutor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RemoteDesktop"
        mc:Ignorable="d"
        Title="UserCommandExecutor" Height="450" Width="400"
        >
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <TextBox Name="txtCommandExecutor" Grid.Row="0" />
        <Button Name="cmdStart" Click="cmdStart_Click" Grid.Row="1" Padding="5">Start</Button>
    </Grid>
</Window>
